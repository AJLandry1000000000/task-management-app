{
  "name": "typescript-task-management",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx tsc && node dist/src/index.js",
    "server": "npx ts-node src/index.ts",
    "migrate:up": "npx ts-node ./node_modules/.bin/knex migrate:up",
    "migrate:down": "npx ts-node ./node_modules/.bin/knex migrate:down",
    "migrate:latest": "npx ts-node ./node_modules/.bin/knex migrate:latest",
    "migrate:list": "npx ts-node ./node_modules/.bin/knex migrate:list",
    "migrate:make": "npx ts-node ./node_modules/.bin/knex migrate:make",
    "seed:make": "npx ts-node ./node_modules/.bin/knex seed:make",
    "seed:run": "npx ts-node ./node_modules/.bin/knex seed:run"
  },
  "keywords": [],
  "author": "Austin Landry",
  "license": "ISC",
  "description": "A task management app write in NodeJS Typescript.",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^2.5.1",
    "knex-paginate": "^3.2.0",
    "pg": "^8.11.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AJLandry1000000000/task-management-app.git"
  }
